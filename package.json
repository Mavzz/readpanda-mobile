  },
  "scripts": {
    "start": "yarn react-native start",
    "pods": "cd ios && pod install && cd ..",
    "clean:all": "yarn clean && cd ios && rm -rf Pods && rm -rf build && cd ..",
    "reset:all": "yarn clean:all && yarn install && yarn pods",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "react-native run-web",
    "clean": "yarn cache clean && watchman watch-del-all && rm -rf node_modules/",
    "reset": "yarn clean && yarn install",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:check": "eslint src/ --max-warnings=0"
  },
